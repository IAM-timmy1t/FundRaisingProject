{
  "name": "blessed-horizon",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-vapid": "node scripts/generate-vapid-keys.js",
    "test:notifications": "node tests/test-notifications.js",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:smoke": "vitest run test/smoke-tests.js",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:unit -- --coverage --reporter=junit --outputFile=test-results/junit.xml",
    "test:coverage": "vitest run --coverage && open coverage/index.html",
    "test:production": "npm run test:smoke -- --url=https://blessed-horizon.com",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "analyze": "vite build --mode analyze",
    "generate:api-docs": "node docs/api/generate-docs.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "@supabase/supabase-js": "2.30.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "emoji-picker-react": "^4.9.2",
    "framer-motion": "^10.16.4",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.285.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.5.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.16.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^1.1.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "vite": "^4.5.14",
    "vite-plugin-compression": "^0.5.1",
    "web-push": "^3.6.7",
    "web-vitals": "^5.0.3"
  }
}
